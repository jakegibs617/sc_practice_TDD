<%= form_for @cell_phone do |f| %>
  <%= f.label :manufacturer_id %>
  <%= f.collection_select(:manufacturer_id, Manufacturer.all, :id, :name) %>
  <br>
  <br>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <br>
  <br>
  <%= f.label :battery_life %>
  <%= f.text_field :battery_life %>
  <br>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <br>
  <%= f.submit "Submit" %>
<% end %>  
